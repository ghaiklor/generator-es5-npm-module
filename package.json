{
  "name": "generator-es5-npm-module",
  "version": "1.1.1",
  "description": "Yeoman generator for starting ES5 npm module with Mocha, Istanbul, Travis, Coveralls",
  "license": "MIT",
  "main": "generators/app/index.js",
  "repository": "ghaiklor/generator-es5-npm-module",
  "author": {
    "name": "Eugene Obrezkov",
    "email": "ghaiklor@gmail.com",
    "url": "https://github.com/ghaiklor"
  },
  "scripts": {
    "clean": "rm -rf ./coverage ./generators",
    "compile": "npm run clean; cp -r src/ generators/; babel src --out-dir generators",
    "prepublish": "npm run test",
    "test": "npm run compile; istanbul cover _mocha"
  },
  "keywords": [
    "yeoman-generator",
    "boilerplate",
    "scaffolder",
    "npm",
    "module",
    "ES5"
  ],
  "dependencies": {
    "yeoman-generator": "0.20.3",
    "yosay": "1.0.5"
  },
  "devDependencies": {
    "babel": "5.8.23",
    "istanbul": "0.3.21",
    "mocha": "2.3.3"
  }
}
